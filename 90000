}elseif ($hari=="Wednesday") {
 echo "Rabu";
}elseif ($hari=="Thursday") {
 echo("Khamis");
}elseif ($hari=="Friday") {
 echo "Jumaat";
}elseif ($hari=="Saturday") {
 echo "Sabtu";
}
?>,

